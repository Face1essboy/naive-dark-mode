{
  "name": "naive-dark-mode",
  "version": "0.1.0",
  "description": "Dark Mode for Vue3 and Naive UI.",
  "private": false,
  "author": "Tohrusky",
  "license": "MIT",
  "main": "./dist/naive-dark-mode.mjs",
  "module": "./dist/naive-dark-mode.mjs",
  "types": "./dist/naive-dark-mode.d.ts",
  "files": [
    "dist/*.mjs",
    "dist/*.d.ts",
    "dist/*.js",
    "dist/*.cjs",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/Tohrusky/naive-dark-mode",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohrusky/naive-dark-mode"
  },
  "keywords": [
    "naive-ui",
    "dark-mode",
    "vue3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && vite build",
    "preview": "pnpm build && vite preview",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck": "pnpm lint && vue-tsc --noEmit",
    "lib": "pnpm typecheck && vite build --mode lib"
  },
  "dependencies": {
    "naive-ui": "^2.34.4",
    "sass": "^1.64.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-core": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vue-tsc": "^1.8.5"
  }
}
