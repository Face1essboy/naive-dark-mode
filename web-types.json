{
  "$schema": "https://raw.githubusercontent.com/JetBrains/web-types/master/schema/web-types.json",
  "framework": "vue",
  "name": "naive-dark-mode",
  "version": "0.2.1",
  "js-types-syntax": "typescript",
  "contributions": {
    "html": {
      "vue-components": [
        {
          "name": "NaiveDarkMode",
          "description": "Dark Mode for Vue3 and Naive UI.",
          "doc-url": "https://github.com/Tohrusky/naive-dark-mode",
          "source": {
            "symbol": "NaiveDarkMode"
          },
          "slots": [],
          "attributes": [],
          "props": [
            {
              "name": "dark-mode",
              "description": "The dark mode setting.",
              "default": "'system'"
            },
            {
              "name": "color",
              "description": "The transition color value.",
              "default": "'#ffffff'"
            },
            {
              "name": "naivetheme",
              "description": "The Naive UI theme.",
              "default": "undefined"
            },
            {
              "name": "fade-layer",
              "description": "The number of steps or iterations for the smooth transition to dark mode. <= 0 for no transition. You can use CSS  transition to control.",
              "default": "25"
            },
            {
              "name": "design-light",
              "description": "The design color for light mode.",
              "default": "'#ffffff'"
            },
            {
              "name": "design-dark",
              "description": "The design color for dark mode.",
              "default": "'#000000'"
            }
          ]
        }
      ]
    }
  }
}
